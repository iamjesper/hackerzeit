{
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "silent": true
  },
  "redirects": [{ "source": "/", "destination": "/best" }],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}
